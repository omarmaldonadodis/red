# .env.example
# Copia este archivo a .env y completa con tus credenciales

# ==========================================
# ADSPOWER
# ==========================================
ADSPOWER_API_URL=http://local.adspower.net:50325
ADSPOWER_API_KEY=

# ==========================================
# SOAX PROXIES
# ==========================================
SOAX_USERNAME=
SOAX_PASSWORD=
SOAX_PROXY_HOST=proxy.soax.com
SOAX_PROXY_PORT=9999

# ==========================================
# 3X-UI (OPCIONAL)
# ==========================================
USE_3XUI=false
THREEXUI_PANEL_URL=
THREEXUI_USERNAME=
THREEXUI_PASSWORD=
THREEXUI_INBOUND_ID=

# ==========================================
# CONFIGURACIÓN GENERAL
# ==========================================
DEFAULT_COUNTRY=ec
DEFAULT_TIMEZONE=America/Guayaquil
DEFAULT_LANGUAGE=es-EC

# ==========================================
# CONFIGURACIÓN DE WARMUP
# ==========================================
WARMUP_MIN_DURATION=30
WARMUP_MAX_DURATION=60
WARMUP_MIN_SITES=8
WARMUP_MAX_SITES=15